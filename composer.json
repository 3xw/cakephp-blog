{
  "name": "3xw/cakephp-blog",
  "description": "CakePHP blog posts cat tags",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=5.6",
    "cakephp/cakephp": "3.5.*",
    "3xw/attachment": "3.5.*"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Trois\\Blog\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trois\\Blog\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
